        RowBox[{"{", 
         RowBox[{"c", ",", "a"}], "}"}]}], "}"}]}], "]"}], "/;", 
    RowBox[{
     RowBox[{
      RowBox[{"Dimensions", "@", "a"}], "\[Equal]", 
      RowBox[{"Dimensions", "@", "b"}], "==", 
      RowBox[{"Dimensions", "@", "c"}]}], "&&", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Length", "[", 
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"NumberQ", "/@", 
          RowBox[{"Flatten", "[", 
           RowBox[{"{", 
            RowBox[{"a", ",", "b", ",", "c"}], "}"}], "]"}]}], ",", "False"}],
         "]"}], "]"}], "\[Equal]", "0"}]}]}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"PointsTriangleQ", "[", "points_List", "]"}], ":=", 
  RowBox[{
