Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"PointsTriangleQ", "[", 
    RowBox[{"a_List", ",", "b_List", ",", "c_List"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"ResourceFunction", "[", "\"\<TriangleEdgesQ\>\"", "]"}], "[", 
     RowBox[{"EuclideanDistance", "@@@", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"a", ",", "b"}], "}"}], ",", 
